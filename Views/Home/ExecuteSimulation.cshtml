@model IEnumerable<_ProyectoFinal_Johanny_Vivas_Arias.Models.DamageLog>

@{
    ViewData["Title"] = "ExecuteSimulation";
}

<h1>SIMULACIÓN</h1>

<p>
    <a asp-action="Index">REGRESAR</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                FALLA
            </th>
            <th>
                FECHA
            </th>
            <th>
                CONTRUIDOS
            </th>
            <th>
                GANANCIAS BRUTAS
            </th>
            <th>
                GANANCIAS REALES
            </th>
            <th>
                FECHA REPARACIÓN
            </th>
          
            <th>
                CONTRUIDOS 
            </th>
            <th>
                GANACIAS BRUTAS 
            </th>
            <th>
                GANACIAS REALES
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DamageMachine.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.month)M/@Html.DisplayFor(modelItem => item.day)D/@Html.DisplayFor(modelItem => item.hour)H
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.day)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.hour)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.buidProdInFailTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.wingrossInFailTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.realWinInFailTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.monthRepare)M/@Html.DisplayFor(modelItem => item.dayRepare)D/@Html.DisplayFor(modelItem => item.hourRepare)H

            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MachineInProd.name)  @Html.DisplayFor(modelItem => item.buidProdOtherMachine) 
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MachineInProd.name) $ @Html.DisplayFor(modelItem =>  item.wingrossOtherMachine)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MachineInProd.name) $ @Html.DisplayFor(modelItem => item.realWinOtherMachine)
            </td>
        </tr>
}
    </tbody>
</table>
